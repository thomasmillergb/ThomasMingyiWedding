	<profiles>
		<!--
			================ Environment Profiles
			================================ Most profiles simply declare a
			single property: target.env This will usually match the Profile name.
			It will be used to source property values from
			/mrMaster/config/{$target.env}.properties The reason we don't use the
			profile name directly is that, sometimes, we may wish to specify more
			than one profile on the mvn command line. (A profile does not
			necessarily need to be a complete environment settings list, e.g.
			cargo profile below.)
			======================================================================
		-->

		<!--
			================= Production Settings Profile
			======================== Use "-P production" when you want to use the
			settings in this profile
			======================================================================
		-->

		<profile>
			<id>production</id>
			<properties>
				<target.env>production</target.env>
				<dbunit.skip>${maven.test.skip}</dbunit.skip>
			</properties>
			<build>
				<plugins>
					<!--
						Override location of data file for DbUnit to load (doesn't have
						negative keys)
					-->
					<plugin>
						<groupId>org.codehaus.mojo</groupId>
						<artifactId>dbunit-maven-plugin</artifactId>
						<configuration>
							<src>src/main/resources/default-data.xml</src>
						</configuration>
					</plugin>
				</plugins>
			</build>
		</profile>

		<!--
			========== End Of Production Settings Profile
			========================
		-->

		<profile>
			<id>devlocal</id>
